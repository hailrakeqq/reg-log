@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <div>
        @if (User.Identity.IsAuthenticated)
        {
            @Html.ActionLink("logout...", "Logoff")
        }
        else
        {
            @Html.ActionLink("login", "Login")
            @Html.Label(" | ")
            ;
            @Html.ActionLink("register", "Register")
        }
    </div>
</div>
</div>